    <managed-bean-class>com.teammates.studentgrademanager.model.AdminLoginSuccessBeanClass</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>UpdateStudentDetailsBean</managed-bean-name>
    <managed-bean-class>com.teammates.studentgrademanager.model.UpdateStudentDetailsBeanClass</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
