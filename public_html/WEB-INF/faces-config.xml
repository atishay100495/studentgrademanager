<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <navigation-rule>
    <from-view-id>/StudentLogin.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/success.jsp</to-view-id>
    </navigation-case>
     <navigation-case>
      <from-outcome>StudentDetails</from-outcome>
      <to-view-id>/StudentDetails.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>fail</from-outcome>
      <to-view-id>/fail.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/RegisterStudent.jsp</from-view-id>
    <navigation-case>
      <from-outcome>successRegister</from-outcome>
      <to-view-id>/successRegister.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>failRegister</from-outcome>
      <to-view-id>/failRegister.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/RegisterAdmin.jsp</from-view-id>
    <navigation-case>
      <from-outcome>successRegisterAdmin</from-outcome>
      <to-view-id>/successRegisterAdmin.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>failRegisterAdmin</from-outcome>
      <to-view-id>/failRegisterAdmin.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/AdminLogin.jsp</from-view-id>
    <navigation-case>
      <from-outcome>successLoginAdmin</from-outcome>
      <to-view-id>/successLoginAdmin.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>failLoginAdmin</from-outcome>
      <to-view-id>/failLoginAdmin.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/successLoginAdmin.jsp</from-view-id>
    <navigation-case>
      <from-outcome>ViewStudentDetails</from-outcome>
      <to-view-id>/ViewStudentDetails.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>ViewSortedList</from-outcome>
      <to-view-id>/ViewStudentDetails.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>UpdateStudentDetails</from-outcome>
      <to-view-id>/UpdateStudentDetails.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/UpdateStudentDetails.jsp</from-view-id>
    <navigation-case>
      <from-outcome>successUpdate</from-outcome>
      <to-view-id>/successUpdate.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>failUpdate</from-outcome>
      <to-view-id>/failUpdate.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <managed-bean>
    <managed-bean-name>LoginBean</managed-bean-name>
    <managed-bean-class>com.teammates.studentgrademanager.model.LoginBeanClass</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <!--oracle-jdev-comment:managed-bean-jsp-link:1Login.jsp-->
  </managed-bean>
  <managed-bean>
