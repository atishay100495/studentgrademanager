  <managed-bean>
    <managed-bean-name>SuccessRegisterBean</managed-bean-name>
    <managed-bean-class>com.teammates.studentgrademanager.model.SuccessRegisterBeanClass</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <!--oracle-jdev-comment:managed-bean-jsp-link:1fail.jsp-->
  </managed-bean>
