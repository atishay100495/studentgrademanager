  <managed-bean>
    <managed-bean-name>AdminLoginBean</managed-bean-name>
    <managed-bean-class>com.teammates.studentgrademanager.model.AdminLoginBeanClass</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
